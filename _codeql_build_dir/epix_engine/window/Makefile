# Special targets to cleanup operation of make.

# Special rule to run CMake to check the build system integrity.
# No rule that depends on this can have commands that come from listfiles
# because they might be regenerated.
cmake_check_build_system:
	cd /home/runner/work/epix_engine/epix_engine/_codeql_build_dir && $(CMAKE_COMMAND) -P /home/runner/work/epix_engine/epix_engine/_codeql_build_dir/CMakeFiles/VerifyGlobs.cmake
	cd /home/runner/work/epix_engine/epix_engine/_codeql_build_dir && $(CMAKE_COMMAND) -S$(CMAKE_SOURCE_DIR) -B$(CMAKE_BINARY_DIR) --check-build-system CMakeFiles/Makefile.cmake 0
.PHONY : cmake_check_build_system

